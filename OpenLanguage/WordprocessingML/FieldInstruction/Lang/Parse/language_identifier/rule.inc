language_identifier:
    whitespace language_identifier { $$ = $2;  $$.LeadingWhitespace.Insert(0, $1); }
  | language_identifier whitespace { $$ = $1; $$.TrailingWhitespace.Add($2); }
  | quote language_identifier quote { $$ = new Quoted<ExpressionNode>($1, $2, $3); }
  | T_HEX_INTEGER                  { $$ = new LanguageIdentifierNode((LanguageIdentifier)(int.Parse($1, NumberStyles.HexNumber, System.Globalization.CultureInfo.InvariantCulture))); }
  | T_BINARY_INTEGER               { $$ = new LanguageIdentifierNode((LanguageIdentifier)(int.Parse($1, NumberStyles.AllowBinarySpecifier | NumberStyles.BinaryNumber, System.Globalization.CultureInfo.InvariantCulture))); }
  | T_INTEGER                      { $$ = new LanguageIdentifierNode((LanguageIdentifier)(int.Parse($1, System.Globalization.CultureInfo.InvariantCulture))); }
  | identifier_or_string_raw       { $$ = new LanguageIdentifierNode(LanguageIdentifierExtensions.FromTag($1), $1); }
  ;

